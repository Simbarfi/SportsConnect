<Window x:Class="SportConnect.MeetupMapWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SportConnect"
        mc:Ignorable="d"
        Title="Meetups" Height="450" Width="800">
    <Grid>
        <Image HorizontalAlignment="Left" Height="379" VerticalAlignment="Top" Width="719" Stretch="UniformToFill" Margin="45,15,0,0" Source="/temp_map.png"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="45" Width="35" Click="Button_Click"/>
        <Label Content="Click an event to join! Click anywhere on the map to start creating an event." HorizontalAlignment="Left" Margin="45,387,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
